<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/normalbgcolor">

    <LinearLayout
        android:id="@+id/editimformation"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/skyblue">
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_centerVertical="true"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:src="@drawable/returnimg" />

            <TextView
                android:id="@+id/editimformation_left_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/editimformation_right_text"
                android:layout_alignBottom="@+id/editimformation_right_text"
                android:layout_toEndOf="@+id/imageView"
                android:text="返回"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/editimformation_right_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="15dp"
                android:text="完成"
                android:textSize="20sp" />
        </RelativeLayout>
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/editimformation"
        android:background="@color/normallinecolor"/>


    <RelativeLayout
        android:id="@+id/autograph"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/editimformation"
        android:layout_alignParentStart="true"
        android:layout_marginTop="0dp"
        android:gravity="center_vertical"
        android:background="@color/white">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:layout_marginLeft="10dp"
            android:text="签名"
            android:id="@+id/autograph1"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp" />
        <EditText
            android:id="@+id/autographtext"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="无咩"
            android:background="@null"
            android:layout_alignBaseline="@+id/autograph1"
            android:layout_alignBottom="@+id/autograph1"
            android:layout_toEndOf="@+id/autograph1"
            android:layout_marginStart="20dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="120dp"
            android:src="@drawable/right"
            android:layout_marginEnd="16dp"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>
    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/autograph"
        android:background="@color/normallinecolor"
         />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/autograph"
        android:background="@color/normallinecolor" />
    <RelativeLayout
        android:id="@+id/username"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="25dp"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_below="@+id/autograph"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:layout_marginLeft="10dp"
            android:text="昵称"
            android:id="@+id/username1"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp" />
        <EditText
            android:id="@+id/usernametext"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="啥玩意啊"
            android:background="@null"
            android:layout_alignBaseline="@+id/username1"
            android:layout_alignBottom="@+id/username1"
            android:layout_toEndOf="@+id/username1"
            android:layout_marginStart="20dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="120dp"
            android:src="@drawable/right"
            android:layout_marginEnd="16dp"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>
    <!--4-->
    <View
    android:id="@+id/view4"
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:layout_below="@+id/username"
    android:background="@color/normallinecolor"/>

    <RelativeLayout
        android:id="@+id/sex"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_below="@+id/view4"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:layout_marginLeft="10dp"
            android:text="性别"
            android:id="@+id/sex1"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp" />
        <Spinner
            android:id="@+id/sextext"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:entries="@array/sex"
            android:textSize="20sp"
            android:text="男"
            android:background="@null"
            android:maxLength="1"
            android:layout_marginStart="20dp"
            android:layout_alignBaseline="@+id/sex1"
            android:layout_alignBottom="@+id/sex1"
            android:layout_toEndOf="@+id/sex1" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="120dp"
            android:src="@drawable/right"
            android:layout_marginEnd="16dp"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>
<!--5-->
    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/sex"
        android:background="@color/normallinecolor"/>
    <!--6-->
    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/sex"
        android:layout_marginTop="24dp"
        android:background="@color/normallinecolor" />
    <RelativeLayout
        android:id="@+id/school"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_below="@+id/sex"
        android:layout_alignParentStart="true"
        android:layout_marginTop="25dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:layout_marginLeft="10dp"
            android:text="学校"
            android:id="@+id/school1"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp" />
        <EditText
            android:id="@+id/schooltext"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="江西师范大学"
            android:background="@null"
            android:layout_alignBaseline="@+id/school1"
            android:layout_alignBottom="@+id/school1"
            android:layout_toEndOf="@+id/school1"
            android:layout_marginStart="20dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="120dp"
            android:src="@drawable/right"
            android:layout_marginEnd="16dp"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/school"
        android:background="@color/normallinecolor"
        android:id="@+id/view7" />
    <RelativeLayout
        android:id="@+id/email"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_below="@+id/view7"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:layout_marginLeft="10dp"
            android:text="邮箱"
            android:id="@+id/email1"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp" />
        <EditText
            android:id="@+id/emailtext"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="无"
            android:background="@null"
            android:layout_alignBaseline="@+id/email1"
            android:layout_alignBottom="@+id/email1"
            android:layout_toEndOf="@+id/email1"
            android:layout_marginStart="20dp" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="120dp"
            android:src="@drawable/right"
            android:layout_marginEnd="16dp"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/email"
        android:background="@color/normallinecolor"
        android:id="@+id/view8" />


